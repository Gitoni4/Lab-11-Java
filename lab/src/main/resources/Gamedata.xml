<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="javalab11.lab.Gamedata">
        <table name="gamedata" schema="game" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="-1"/>
            </id>
            <basic name="numberOfPlayers">
                <column name="number_of_players" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
