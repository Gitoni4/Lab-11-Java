<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="javalab11.lab.Players">
        <table name="players" schema="game" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" length="100"/>
            </basic>
            <basic name="gameId">
                <column name="game_id" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
